{
  "name": "dependency-lint",
  "version": "5.0.1",
  "description": "Lints package.json dependencies and devDependencies",
  "main": "index.js",
  "bin": {
    "dependency-lint": "./bin/dependency-lint.js"
  },
  "scripts": {
    "build": "gulp",
    "cucumber-test": "cucumber-js",
    "lint-coffeescript": "coffeelint src -q",
    "feature-test": "yarn run cucumber-test && yarn run self-test",
    "prefeature-test": "yarn run build",
    "prepublish": "yarn run build",
    "pretest": "yarn run lint-coffeescript",
    "self-test": "./bin/dependency-lint.js",
    "test": "yarn run unit-test && yarn run feature-test",
    "unit-test": "mycha --reporter dot"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/charlierudolph/dependency-lint.git"
  },
  "keywords": [
    "dependency",
    "devDependency",
    "lint"
  ],
  "author": {
    "name": "Charlie Rudolph",
    "email": "charles.w.rudolph@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/charlierudolph/dependency-lint/issues"
  },
  "homepage": "https://github.com/charlierudolph/dependency-lint",
  "devDependencies": {
    "babel-core": "^6.17.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.16.0",
    "chai": "^3.0.0",
    "chai-as-promised": "^6.0.0",
    "coffee-script": "^1.9.0",
    "coffeelint": "^1.8.1",
    "cucumber": "2.0.0-rc.1",
    "gulp": "^3.9.0",
    "gulp-changed": "^1.3.0",
    "gulp-coffee": "^2.3.1",
    "is-generator": "^1.0.3",
    "mycha": "^3.0.0",
    "sinon": "^1.14.1",
    "sinon-chai": "^2.8.0",
    "tmp": "0.0.31"
  },
  "dependencies": {
    "bluebird": "^3.4.3",
    "builtin-modules": "^1.1.1",
    "camel-case": "^3.0.0",
    "colors": "^1.0.3",
    "detective": "^4.0.0",
    "docopt": "^0.6.0",
    "fs-extra": "^1.0.0",
    "glob": "^7.0.0",
    "js-yaml": "^3.3.1",
    "lodash": "^4.2.1",
    "minimatch": "^3.0.2",
    "semver": "^5.1.0",
    "sorted-object": "^2.0.0"
